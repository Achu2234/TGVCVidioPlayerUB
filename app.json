  "repository": "https://github.com/Achu2234/TGVCVidioPlayerUBt",
  "logo": "https://telegra.ph/file/a334fb98f077f132715ea.jpg",
  "keywords": ["python", "telegram", "bot", "video"],
  "env": {
    "API_ID": {
      "description": "API_ID get it from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "API_HASH get it from my.telegram.org/apps",
      "required": true
    },
    "SESSION_STRING": {
      "description": "Pyrogram Session String, get it from repel",
      "required": true
    },
    "LOG_ID": {
      "description": "ID of chat for logs",
      "required": true
 }
    },
    "formation": {
      "worker": {
        "quantity": 1,
